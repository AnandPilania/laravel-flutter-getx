{
    "name": "anandpilania/laravel-flutter-getx",
    "description": "Laravel package that easily scaffold Flutter project as Laravel structure & GetX",
    "keywords": ["laravel", "getx", "flutter", "scaffold", "php-laravel", "anandpilania"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pilaniaanand/laravel-flutter-getx/issues",
        "source": "https://github.com/pilaniaanand/laravel-flutter-getx"
    },
    "authors": [
        {
            "name": "Anand Pilania",
            "email": "pilaniaanand@gmail.com"
        }
    ],
    "require": {
		"php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "LaravelFlutterGetx\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "LaravelFlutterGetx\\ServiceProvider"
            ]
        }
    },
	"bin": [
        "bin/laravel-flutter-getx"
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
